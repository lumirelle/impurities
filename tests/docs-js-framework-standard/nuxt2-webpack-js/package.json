{
  "name": "nuxt2-webpack-js",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^14.21.3 || ^16.10.0 || >=18.0.0",
    "npm": ">=6"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^18.20.0 || ^20.10.0 || >=21.0.0",
      "onFail": "error"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "pnpm lint:js",
    "lintfix": "pnpm lint:js -- --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.44.0",
    "element-ui": "^2.15.14",
    "node-fetch-native": "1.6.4",
    "nuxt": "2.17.3",
    "vue": "^2.7.16",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~4.14.1",
    "@nuxt/types": "2.17.3",
    "eslint": "^9.32.0",
    "lint-staged": "^15.5.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "~5.8.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  }
}
