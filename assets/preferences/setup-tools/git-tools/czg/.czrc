{
  "$schema": "https://raw.githubusercontent.com/Zhengqbbb/cz-git/refs/tags/v1.12.0/docs/public/schema/cz-git.json",
  "markBreakingChangeMode": true,
  "allowBreakingChanges": [ "feat", "fix", "chore" ],
  "alias": {
    "typo": "docs: fix typos",
    "readme": "docs: update README.md"
  },
  "scopeOverrides": {
    "test": [
      "unit-test",
      "e2e-test"
    ],
    "chore": [
      "assets",
      "deps",
      "tools"
    ]
  }
}
